/*图片一像素边框 */

.img_box {
    width: 250rpx;
    height: 250rpx;
    position: relative;
}

.img_box::after {
    content: "";
    box-sizing: border-box;
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 1px solid #c9c9c9;
    transform: scale(0.5);
    transform-origin: 0 0;
    border-radius: 5px;
}

.img_box_2::after {
    content: "";
    box-sizing: border-box;
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 1px solid #f90;
    transform: scale(0.5);
    transform-origin: 0 0;
    border-radius: 5px;
}


