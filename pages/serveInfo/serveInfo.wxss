/* 头部开始 */

.header {
  height: 432rpx;
  position: relative;
  overflow: hidden;
}

.header::before {
  content: '';
  height: 50%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -10;
  background-color: #6747ce;
}

.header::after {
  content: '';
  height: 50%;
  width: 100%;
  position: absolute;
  left: 0;
  z-index: -10;
  bottom: 0;
  background-color: #fff;
}

.header-userBox {
  margin-top: 170rpx;
  margin-left: 90rpx;
  overflow: hidden;
}

.header-userImg {
  border-radius: 50%;
  border: 11rpx solid #6747ce;
  height: 160rpx;
  width: 160rpx;
  overflow: hidden;
}

.header-userImg > image {
  height: 100%;
  width: 100%;
}

.header-user {
  margin-left: 170rpx;
  display: flex;
  flex-direction: column;
}

.header-user-name {
  font-size: 34rpx;
  color: #fff;
}

.serveIcon {
  margin: 24rpx 0;
}

.serveIcon image {
  width: 32rpx;
  margin-left: 5rpx;
  height: 32rpx;
}


.attention button {
  float: left;
  width: 241rpx;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 28rpx;
  color: #fff;
  border-radius: 50px;
  background-color: #6a47ff;
}

.serveNum {
  overflow: hidden;
  position: absolute;
  bottom: 0;
  width: 100%;
}

.serveNum-li {
  font-size: 24rpx;
  text-align: center;
  width: 20%;
  height: 70rpx;
  line-height: 70rpx;
  float: left;
  color: #999;
}

.serveNum-li text:last-child {
  color: #6c4afd;
  font-weight: bold;
  margin-left: 10rpx;
}

/* 头部结束 */

/* 荣誉资质 start */

.scrol-wrap {
  background-color: #fff;
  padding: 12rpx 12rpx;
  overflow: hidden;
  margin: 10rpx 0;
}

.scrol-wrap-title {
  font-size: 30rpx;
  padding-left: 12rpx;
  font-weight: bold;
  border-left: 6rpx solid #6c4afd;
}

.scrol-wrap-content {
  display: flex;
  padding: 20rpx 10rpx;
  flex-direction: row;
  width: 5600rpx;
}

.scrol-wrap-content > view {
  width: 672rpx;
  overflow: hidden;
  padding: 14rpx;
  margin-right: 15rpx;
  height: 212rpx;
  border-radius: 5px;
  box-shadow: 0px 0px 14rpx 2rpx rgba(0, 0, 0, 0.2);
}

.scrol-wrap-content-title {
  color: #404040;
  font-size: 26rpx;
  margin-bottom: 15rpx;
}

.scrol-wrap-content > view > image {
  width: 130rpx;
  height: 130rpx;
  float: left;
}

.scrol-wrap-content-r {
  margin-left: 150rpx;
}

.scrol-wrap-content-r-t {
  color: #595959;
  font-size: 24rpx;
}

.scrol-wrap-content-r-b {
  overflow: hidden;
  margin-top: 5rpx;
}

.scrol-wrap-content-r-b > text {
  font-size: 18rpx;
  color: #595959;
}

/* 荣誉资质 end */

/* 其他服务 start */
.other-service{
  background-color: #fff;
  margin: 20rpx 0;
  padding: 12rpx;
}
.other-service-li{
  position: relative;
  margin-top: 200rpx;
  
  z-index: 1;

}
.other-service-title{
  border: 1px solid red;
  display: flex;
  width: 123rpx;
  position: absolute;
  background-color: #fff;
  top: -150rpx;
  z-index: -1;
  height: 243rpx;
  border-radius: 40rpx;
  flex-direction: column;
  align-items: center
}
.other-service-title image{
  width: 70rpx;
  margin-top: 18rpx;
  height: 70rpx;
}


.other-service-title text{
  font-size: 24rpx;
  margin-top: 16rpx;
  color: #404040;
}

.other-service-cont{
  border: 1px solid blue;
  
}
/* 其他服务 end */





/* 服务方式 start */

.serveManner {
  background-color: #fff;
  padding: 30rpx 0 40rpx;
  border-bottom: 1px solid #f5f5f5;
}

.h1 {
  font-size: 30rpx;
  font-weight: bold;
  padding-left: 12rpx;
  margin-left: 16rpx;
  border-left: 6rpx solid #6c4afd;
}
.h2 {
  font-size: 30rpx;
  font-weight: bold;
  padding-left: 12rpx;
}

.serveLabel {
  font-size: 26rpx;
  color: #595959;
  overflow: hidden;
}

.serveLabel text {
  padding: 0 10rpx;
  height: 48rpx;
  line-height: 48rpx;
  background-color: #6C4AFD;
  color: #fff;
  text-align: center;
  border-radius: 6rpx;
  float: left;
  margin-top: 20rpx;
  margin: 20rpx 10rpx 0;
}
/* .serveLabel text:last-child{
  
  margin-right: 0;
} */

/* 服务方式 end */

/* 服务内容 start */

.serveCont {
  padding: 20rpx 36rpx 0;
  line-height: 45rpx;
  color: #595959;
  font-size: 26rpx;
}

/* 服务内容 end */

