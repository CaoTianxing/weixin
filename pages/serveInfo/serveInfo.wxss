/* 页脚 start */

.footer {
  height: 110rpx !important;
  position: fixed;
  bottom: 0;
  overflow: hidden;
  padding: 22rpx 24rpx;
  box-sizing: border-box;
  width: 100%;
  /* z-index: 1000000; */
  background-color: #fff;
  box-shadow: 0rpx 0rpx 30rpx 2rpx rgba(0, 0, 0, 0.2);
}

.footer-l {
  float: left;
  font-size: 28rpx;
  color: #404040;
  font-weight: bold;
  position: relative;
  z-index: 10000;
}

.gps {
  font-size: 20rpx;
  color: #808080;
  font-weight: normal;
  margin-top: 10rpx;
}

.gps cover-image {
