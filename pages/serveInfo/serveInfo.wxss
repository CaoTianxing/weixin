}

/* 视频介绍 end */

/* 图片介绍 start */

.allServeImg {
  float: right;
  margin-top: 8rpx;
  margin-right: 30rpx;
}

.righticon {
  margin-top: 2rpx;
  float: right;
  color: #bfbfbf;
}

navigator {
  color: #bfbfbf;
}

.serveImgBox {
  padding: 0 30rpx;
  display: flex;
  font-size: 22rpx;
  margin-top: 24rpx;
}

.serveImgBox image {
  width: 220rpx;
  height: 220rpx;
  margin-left: 15rpx;
}

.serveImgBox image:first-child {
  margin-left: 0;
}

/* 图片介绍 end */

/* 其他服务 start */

.nav {
  padding: 34rpx 0;
  background-color: #fff;
}

.nav-ul {
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
}

.mt28 {
  margin-top: 28rpx;
}

.navtext {
  font-size: 24rpx;
  color: #404040;
  margin-top: 16rpx;
}

.nav-li {
  width: 25%;
  box-sizing: border-box;
  text-align: center;
}

.nav-li image {
  width: 70rpx;
  height: 70rpx;
}

.nav-li > view {
  margin-top: 16rpx;
}

/* 其他服务 end */

/* 评价 start */

.evaluate {
  overflow: hidden;
  width: 650rpx;
  margin: auto;
  padding: 46rpx 0 30rpx;
  border-bottom: 1px solid #e6e6e6;
}

.evaluate-l {
  width: 314rpx;
  float: left;
}

.leftGrade {
  display: block;
  font-size: 28rpx;
  color: #404040;
}

.fen {
  margin-top: 40rpx;
}

.GradeNum {
  text-align: center;
}

.FF4040 {
  color: #ff4040;
}

.evaluationBox {
  position: relative;
}

.evaluationRes {
  position: absolute;
  top: 0;
  left: 0;
  padding-left: 96rpx;
  margin-top: 34rpx;
}

.evaluationRes image, .evaluate-r image {
  width: 30rpx;
  height: 30rpx;
}

.evaluationRes image {
  margin: 0 3rpx;
