  font-weight: bold;
  float: left;
  width: 50%;
}

.schoolDate {
  color: #595959;
  margin-top: 38rpx;
  float: left;
  text-align: right;
  font-size: 26rpx;
  width: 50%;
}

.Education {
  width: 100%;
  margin-top: 20rpx;
}

.Education text {
  margin-left: 20rpx;
}

/* 教育经历 end */

/* 视频介绍 start */

.videoBox {
  overflow: hidden;
  padding: 0 30rpx;
}

video {
  /* z-index: -1 */
}

.videoBox video {
  border-radius: 6rpx;
  margin-top: 28rpx;
