page{
  background-color: #fff
}
.pageBox{
  position: absolute;
  height: 100%;
  padding-bottom: 100rpx;
}
.pageBoxIn{
  position: fixed;
  width: 100%;
  height: 246rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}
.release{
  display: flex;
  width: 650rpx;
  height: 130rpx;
  margin: auto;
}
.page-l image,.page-r image{
  width: 130rpx;
   height: 130rpx;
}

.page-l{
  flex: 1;
  text-align: center
}
.page-r{
  flex: 1;
  text-align: center
}


.title{
  margin-top: 40rpx;
  font-size: 32rpx;
  color: #595959;
  font-weight: bold
}
.subTitle{
  margin-top: 5rpx;
  font-size: 26rpx;
  color:#BFBFBF
}
