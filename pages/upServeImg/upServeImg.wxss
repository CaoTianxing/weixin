}
page {
  background-color: #fff;
}

.title {
  padding-top:58rpx;
  text-align: center;
  font-size: 34rpx;
  color: #3775f5;
  font-weight: bold;
}

.title text {
  font-size: 34rpx;
  font-weight: bold;
}

.title-btm {
  margin-top: 30rpx;
